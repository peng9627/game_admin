<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field">
    <class name="game.domain.model.recharge.RechargeGive" table="t_recharge_give" lazy="true">
        <id name="id" type="java.lang.String" column="id" length="32">
            <generator class="uuid"/>
        </id>

        <version column="version" name="version" type="java.lang.Integer"/>

        <property name="createDate" column="create_date" type="java.util.Date"/>

        <property name="money" column="money" type="java.math.BigDecimal"/>

        <property name="giveMoney" column="give_money" type="java.math.BigDecimal"/>
    </class>
</hibernate-mapping>