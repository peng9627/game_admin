<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping default-access="field">
    <class name="niuniu.domain.model.seal.Seal" table="t_seal">
        <id name="id" type="java.lang.String" column="id" unsaved-value="null" length="32">
            <generator class="uuid"/>
        </id>

        <version name="version" type="java.lang.Integer" column="version"/>

        <property name="createDate" column="create_date" type="java.util.Date"/>

        <property name="type" column="type">
            <type name="org.hibernate.type.EnumType">
                <param name="enumClass">niuniu.core.enums.SealType</param>
                <param name="type">4</param>
            </type>
        </property>
        <property name="sealNo" column="seal_no" type="java.lang.String"/>
    </class>

</hibernate-mapping>